// 1. Tankerkönig installieren und einrichten.
// 2. Variable als Logikwert erstellen, diese agiert als Trigger für das Skript und wird von Alexa gesteuert,
//    bei mir heist diese 'admin.0.info.Billigste_Tankstelle_Saarbrücken'

on({id: 'admin.0.info.Billigste_Tankstelle_Saarbrücken', val: true}, function (obj) {
  var value = obj.state.val;
  var oldValue = obj.oldState.val;
  if (getState("tankerkoenig.2.stations.cheapest.diesel.name").val != getState("tankerkoenig.2.stations.cheapest.e10.name").val) {
    setState("alexa2.0.Echo-Devices.G090U50683540MQV.Commands.speak"/*speak*/, (['Diesel tankst du am billigsten an der ',getState("tankerkoenig.2.stations.cheapest.diesel.name").val,' für ',getState("tankerkoenig.2.stations.cheapest.diesel.short").val,'Euro . E 10 kostet an der ',getState("tankerkoenig.2.stations.cheapest.e10.name").val,' aktuell ',getState("tankerkoenig.2.stations.cheapest.e10.short").val,' Euro.'].join('')));
  } else {
    setState("alexa2.0.Echo-Devices.G090U50683540MQV.Commands.speak"/*speak*/, (['Am besten fährst du an die ',getState("tankerkoenig.2.stations.cheapest.diesel.name").val,'. dort tankst du Diesel für ',getState("tankerkoenig.2.stations.cheapest.diesel.short").val,'Euro und E 10 für  ',getState("tankerkoenig.2.stations.cheapest.e10.short").val,' Euro.'].join('')));
  }
  getState("admin.0.info.Billigste_Tankstelle_Saarbrücken", function (err, state) {
      setState("admin.0.info.Billigste_Tankstelle_Saarbrücken"/*Billigste Tankstelle Saarbrücken*/, state ? !state.val : true);
  });
});


